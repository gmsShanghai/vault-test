<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <title>ACDC Vault Testing</title>

        <link
            rel="stylesheet"
            href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css"
        />
        <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>
    </head>
    <body>
        <br />

        <div class="ui container">
            <%- include('vault_common_part', { clientId, clientToken,
            PAYPAL_TEST_CARD_NO, PAYPAL_TEST_CARD_DATE, PAYPAL_TEST_CARD_CVV,
            VAULT_MODEL, id_token, CUSTOMER_ID, VAULT_ID, TEST_MERCHANT_ID,
            is_use_PAYPAL_AUTH_ASSERTION, IS_VAULT_OPTION_DISPLAY:true,HAS_ORDER_AMOUNT_PART:true }) %>
            <!-- <%- include('spb_display_area')%> -->

            <div class="ui bottom attached segment">
                <div>Last Digits: <span id="last_digit"></span></div>

                <div>Expiry Date: <span id="expiry_date"></span></div>

                <div>Card Brand: <span id="card_brand"></span></div>

                <div>
                    Card Type: <span id="card_type"></span>

                    <br />
                    <button id="submit" type="button" class="ui primary button">
                        Pay with &nbsp; &nbsp;&nbsp;
                        <i class="cc visa icon big"></i> End with ****1091
                    </button>
                    <br />
                    <!-- <i class="cc visa icon large" ></i>  -->
                </div>
            </div>

            <p id="result-message" class="ui container"></p>
        </div>

        <script
            src="https://www.paypal.com/sdk/js?components=buttons,card-fields&client-id=<%= clientId %>&currency=USD"
            data-user-id-token="<%= id_token %>"
        ></script>
        <script type="module" src="checkout_ACDC_returning.js"></script>
        <script></script>
    </body>
</html>
